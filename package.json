{
  "description": "starter structure for reactjs projects",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker": "docker-compose -f docker/dev.docker-compose.yml up -d",
    "docker:build": "docker-compose -f docker/dev.docker-compose.yml up --build -d",
    "docker:bash": "docker exec -it react bash",
    "docker:logs": "docker logs react -t -f",
    "docker:stop": "docker-compose -f docker/dev.docker-compose.yml down"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "2.11.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/data-grid": "4.0.0-alpha.37",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@material-ui/pickers": "3.3.10",
    "@types/lodash": "4.14.177",
    "date-fns": "2.27.0",
    "font-awesome": "4.7.0",
    "lodash": "4.17.21",
    "node-sass": "6.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.0.2",
    "react-scripts": "4.0.3",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "typescript": "4.5.2"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.20",
    "@types/react-router-dom": "5.3.2",
    "@types/redux-thunk": "2.1.0"
  }
}
