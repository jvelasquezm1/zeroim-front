{
	"name": "react-js-starter",
	"description": "starter structure for reactjs projects",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"docker": "docker-compose -f docker/dev.docker-compose.yml up -d",
		"docker:build": "docker-compose -f docker/dev.docker-compose.yml up --build -d",
		"docker:bash": "docker exec -it react bash",
		"docker:logs": "docker logs react -t -f",
		"docker:stop": "docker-compose -f docker/dev.docker-compose.yml down"
	},
	"dependencies": {
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-router-dom": "^5.1.2"
	},
	"devDependencies": {
		"@types/jest": "24.0.19",
		"@types/node": "12.11.1",
		"@types/react": "16.9.9",
		"@types/react-dom": "16.9.2",
		"react-scripts": "3.2.0",
		"typescript": "3.6.4"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://gitlab.com/wecraftbe/koa-js-starter.git"
	},
	"bugs": {
		"url": "https://gitlab.com/wecraftbe/koa-js-starter/issues"
	},
	"homepage": "https://gitlab.com/wecraftbe/koa-js-starter#readme"
}