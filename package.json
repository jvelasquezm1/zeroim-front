{
  "description": "starter structure for reactjs projects",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker": "docker-compose -f docker/dev.docker-compose.yml up -d",
    "docker:build": "docker-compose -f docker/dev.docker-compose.yml up --build -d",
    "docker:bash": "docker exec -it react bash",
    "docker:logs": "docker logs react -t -f",
    "docker:stop": "docker-compose -f docker/dev.docker-compose.yml down"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.19",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@types/lodash": "^4.14.168",
    "date-fns": "^2.17.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.20",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.19",
    "@types/node": "12.11.1",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-thunk": "^2.1.0",
    "node-sass": "^4.14.1",
    "react-scripts": "^4.0.2",
    "typescript": "3.6.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
